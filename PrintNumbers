#include<iostream>
using namespace std;

int main(){
    int n;
    cin >> n;
  
    print(n);
}

int print(int n) {
  /* Don't write main().
     Don't read input, it is passed as function argument.
     Return output and don't print it.
     Taking input and printing output is handled automatically.
  */
int pr;
        if(n==0)
            return 0;
    pr=1+print(n-1);
    cout<<pr<<" ";
    return pr;
}

